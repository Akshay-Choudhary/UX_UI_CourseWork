import junit.framework.Test;
import junit.framework.TestSuite;

public class SearchFunctionalityAndAccordion {

  public static Test suite() {
    TestSuite suite = new TestSuite();
    suite.addTestSuite(CommunityPortal.class);
    suite.addTestSuite(AboutUsAndSelectOption.class);
    suite.addTestSuite(SearchFunctionalityAndAccordion.class);
    return suite;
  }

  public static void main(String[] args) {
    junit.textui.TestRunner.run(suite());
  }
}
